# Staging Environment Variables
NODE_ENV=production

# Analytics (disabled in staging)
NEXT_PUBLIC_ANALYTICS_URL=
NEXT_PUBLIC_ANALYTICS_SITE_ID=
NEXT_PUBLIC_ANALYTICS_ENABLED=false

# Contact Form
RESEND_API_KEY=
CONTACT_EMAIL=staging@your-domain.com
CONTACT_FROM_EMAIL=noreply-staging@your-domain.com

# Security
NEXTAUTH_SECRET=staging-secret-key
NEXTAUTH_URL=https://staging.your-domain.com

# Performance
NEXT_PUBLIC_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_ERROR_REPORTING=true

# Feature Flags
NEXT_PUBLIC_3D_ENABLED=true
NEXT_PUBLIC_ANALYTICS_DASHBOARD=true

# CDN and Assets
NEXT_PUBLIC_CDN_URL=https://staging-cdn.your-domain.com
NEXT_PUBLIC_ASSET_PREFIX=

# Rate Limiting (more lenient for testing)
RATE_LIMIT_WINDOW_MS=300000
RATE_LIMIT_MAX_REQUESTS=10

# Monitoring
SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=