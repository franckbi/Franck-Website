# Production Environment Variables
NODE_ENV=production

# Analytics
NEXT_PUBLIC_ANALYTICS_URL=https://plausible.io/js/script.js
NEXT_PUBLIC_ANALYTICS_SITE_ID=your-domain.com
NEXT_PUBLIC_ANALYTICS_ENABLED=true

# Contact Form
RESEND_API_KEY=
CONTACT_EMAIL=contact@your-domain.com
CONTACT_FROM_EMAIL=noreply@your-domain.com

# Security
NEXTAUTH_SECRET=
NEXTAUTH_URL=https://your-domain.com

# Performance
NEXT_PUBLIC_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_ERROR_REPORTING=true

# Feature Flags
NEXT_PUBLIC_3D_ENABLED=true
NEXT_PUBLIC_ANALYTICS_DASHBOARD=false

# CDN and Assets
NEXT_PUBLIC_CDN_URL=https://cdn.your-domain.com
NEXT_PUBLIC_ASSET_PREFIX=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=5

# Monitoring
SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=